{
  "name": "tictactoe",
  "version": "0.1.0",
  "description": "The default template project for [nano-react-app](https://github.com/adrianmcli/nano-react-app).",
  "homepage": "https://siddhant-ahire.github.io/tic-toc-toe",
  "scripts": {
    "start": "parcel index.html",
    "build": "npm run clear && parcel build index.html",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:lint": "npm run format && npm run lint",
    "clear": "rimraf dist .parcel-cache",
    "predeploy":"npm run build",
    "deploy":"gh-pages -d dist"
  },
  "license": "MIT",
  "dependencies": {
    "node-sass": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-nano-react-app": "^0.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "parcel": "^1.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ]
  },
  "main": "index.js",
  "author": ""
}
